<p id="notice"><%= notice %></p>

<h1>Calendar Discussions</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @calendar_discussions.each do |calendar_discussion| %>
      <tr>
        <td><%= link_to 'Show', calendar_discussion %></td>
        <td><%= link_to 'Edit', edit_calendar_discussion_path(calendar_discussion) %></td>
        <td><%= link_to 'Destroy', calendar_discussion, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Calendar Discussion', new_calendar_discussion_path %>
