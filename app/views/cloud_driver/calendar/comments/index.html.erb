<p id="notice"><%= notice %></p>

<h1>Calendar Comments</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @calendar_comments.each do |calendar_comment| %>
      <tr>
        <td><%= link_to 'Show', calendar_comment %></td>
        <td><%= link_to 'Edit', edit_calendar_comment_path(calendar_comment) %></td>
        <td><%= link_to 'Destroy', calendar_comment, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Calendar Comment', new_calendar_comment_path %>
