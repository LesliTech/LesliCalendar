<p id="notice"><%= notice %></p>

<h1>Calendar Subscribers</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @calendar_subscribers.each do |calendar_subscriber| %>
      <tr>
        <td><%= link_to 'Show', calendar_subscriber %></td>
        <td><%= link_to 'Edit', edit_calendar_subscriber_path(calendar_subscriber) %></td>
        <td><%= link_to 'Destroy', calendar_subscriber, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Calendar Subscriber', new_calendar_subscriber_path %>
