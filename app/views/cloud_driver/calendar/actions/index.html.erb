<p id="notice"><%= notice %></p>

<h1>Calendar Actions</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @calendar_actions.each do |calendar_action| %>
      <tr>
        <td><%= link_to 'Show', calendar_action %></td>
        <td><%= link_to 'Edit', edit_calendar_action_path(calendar_action) %></td>
        <td><%= link_to 'Destroy', calendar_action, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Calendar Action', new_calendar_action_path %>
